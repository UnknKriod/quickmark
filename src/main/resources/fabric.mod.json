{
  "schemaVersion": 1,
  "id": "quickmark",
  "version": "${version}",
  "name": "QuickMark",
  "description": "A mod that adds the teams and mark system from Fortnite",
  "authors": [
    {
      "name": "UnknKriod",
      "contact": {
        "discord": "unknkriod"
      }
    }
  ],
  "contact": {},
  "license": "MIT",
  "icon": "assets/quickmark/logo.png",
  "environment": "*",
  "entrypoints": {
    "client": ["me.unknkriod.quickmark.Quickmark"],
    "server": ["me.unknkriod.quickmark.QuickmarkServer"]
  },
  "custom": {
    "fabric-networking-api-v1:optional": true
  },
  "mixins": [
    {
      "config": "quickmark.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": ">=${minecraft_version}"
  }
}
